	<xsd:complexType name="SW_DividendSwapDetails">
		<xsd:annotation>
			<xsd:documentation>Additional trade details specific to the 'Share Dividend Swap' and 'Index Dividend Swap' products.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="numberOfShares" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>In respect of a Dividend Swap transaction, the number of shares/baskets applicable to the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swStartDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>On a Dividend Swap transaction, represents the date of the start of the first dividend period on the trade </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swLastDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>On a Dividend Swap transaction, represents the date of the end of the last dividend period on the trade</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swBreakOutTrade" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">A component for determining if the Dividend Swap trade should be broken out into child trades upon affirmation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swSplitCollateral" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">A component for determining if collateral payments should all be represented on the first broken out child.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swDividendSwapDecrease" type="SW_DividendSwapDecrease" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">A component for defining details of a decrease on a Dividend Swap transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swDividendSwapPeriodDetails" type="SW_DividendSwapPeriodDetails" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A component to hold additional dividend period details for Japanese Dividend Swaps</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swPeriodPaymentDate" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A component to hold the Period Payment Date for Dividend Swap Transactions other than Japanese Dividend Swap Transactions</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:date">
							<xsd:attribute name="href"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SW_DividendSwapPeriodDetails">
		<xsd:sequence>
			<xsd:element name="swDividendAmountPaymentDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>For a Japanese Dividend Swap Transaction, the Dividend Amount Payment date for the relevant Leg of the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swFixedAmountPaymentDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>For a Japanese Dividend Swap Transaction, the Fixed Amount Payment date for the relevant Leg of the transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="href">
			<xsd:annotation>
				<xsd:documentation>Relative to dividendPeriod element</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="SW_DividendSwapDecrease">
		<xsd:annotation>
			<xsd:documentation>A structure to hold the details of a decrease on a dividend swap transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="tradeDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>The trade date of the decrease</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swPreviousOpenUnits" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>For a Dividend Swap transaction, the number of shares/baskets prior to the decrease occurring.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swDecreaseOpenUnits" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>For a Dividend Swap transaction, the implied decrease in the number of shares/baskets as a result of the decrease.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swPositionChangeFeeDetails" type="SW_PositionChangeFeeDetails" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Fee payments supporting payment direction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SW_EquityOptionStrategy">
		<xsd:annotation>
			<xsd:documentation>A structure to hold the details of a Equity Option Strategies.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="swStrategy">
				<xsd:annotation>
					<xsd:documentation>A structure to hold supplementary data regarding Strategy trades.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="swStrategyType" type="xsd:normalizedString">
							<xsd:annotation>
								<xsd:documentation>Field indicates the Strategy type and much match one of the predesignated values.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="swStrategyParent" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates the Swapswire Trade ID of the original parent trade.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="swStrategyLegId" type="xsd:normalizedString" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Contains the leg identifier of the trade, carried over from the original parent trade.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="swStrategyComments" type="xsd:normalizedString" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Bilaterally agreed comments pertaining to the Strategy trade.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						
						<xsd:element name="swSingleStrategy" type="xsd:boolean" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Specify to keep strategy as single trade (true) or split into child trades (false)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="swEquityOptionStrategyDetails" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The swEquityOptionStrategyDetails structure contains core financial data for supplementary strategy legs.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="SW_EquityOptionStrategyDetails">
							<xsd:attribute name="id"/>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SW_EquityOptionStrategyDetails">
		<xsd:annotation>
			<xsd:documentation>A structure to hold the details of a decrease on a dividend swap transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="BuyerSeller.model"/>
			<xsd:element name="numberOfOptions" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>The number of options comprised in the option transaction. Reuse of 4.1 FpmL element.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="optionType" type="OptionTypeEnum">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The type of option transaction. Reuse of 4.1 FpmL element.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="strike" type="EquityStrike">
				<xsd:annotation>
					<xsd:documentation>Defines whether it is a price or level at which the option has been, or will be, struck.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pricePerOption" type="Money">
				<xsd:annotation>
					<xsd:documentation>The Price Per Option.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swAveragingDates" type="SW_AveragingDates" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">A component for defining additional equity option details for Averaging Dates.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="expirationDate" type="AdjustableOrRelativeDate">
				<xsd:annotation>
					<xsd:documentation>The last day within an exercise period for an American style option. For a European style option it is the only day within the exercise period.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="paymentAmount" type="Money">
				<xsd:annotation>
					<xsd:documentation>The equity option premium payable by the buyer to the seller.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swEquityOptionPercentInput" type="SW_EquityOptionPercentInput" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Notional input parameters related to strategy legs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="swNovations">
		<xsd:annotation>
			<xsd:documentation>A component for defining details of subtrades involved in a block novation.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="novation" type="SW_ConfirmedNovationAgreement" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SW_DispersionVarianceSwapDetails">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Additional trade details specific to the 'Dispersion Variance Swap' product.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="swShareComponentCount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Provides a count of the number of share component legs associated with the Dispersion Variance Swap.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swVarianceSwapShareHolidayDate" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>An array of holidays for the exchange on which the underlyer trades that will occur during the term of the variance swap, agreed at the commencement of the trade, and used in the calculation of the Expected N. This is a cumulative list of all shre leg holidays.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swLegComponentDetails" type="SW_LegComponentDetails" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Provides additional information related to each component leg associated with the Dispersion Variance Swap.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swTotalVarianceCap" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The full index leg variance cap. The variance cap is equal to unadjustedVarianceCap^ * the variance strike price.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swTotalVolatilityCap" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The full index leg volatility cap. The volatility cap is equal to unadjustedVarianceCap * the volatility strike price.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swDispersionVarianceSwapTermination" type="SW_DispersionVarianceSwapTermination" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Details of a Dispersion Variance Swap Termination Event.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swDispersionIndexCancellationDetails" type="SW_DispersionIndexCancellationDetails" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Details of a Dispersion Variance Swap Index Cancellation Event.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SW_LegComponentDetails">
		<xsd:annotation>
			<xsd:documentation>A structure to hold the details of a decrease on a dividend swap transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="volatilityStrikePrice" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The volatility strike price. The volatility strike is the square root of the variance strike.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="swExpectedNOverride" type="xsd:boolean">
				<xsd:annotation>
