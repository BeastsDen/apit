<?xml version="1.0"?>
<xsd:schema xmlns="http://www.fpml.org/2005/FpML-4-2" xmlns:swrml="http://www.swapswire.com/.../reporting-0-1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="SW_20_1_460930" targetNamespace="http://www.fpml.org/2005/FpML-4-2" elementFormDefault="qualified" attributeFormDefault="unqualified">
<xsd:include schemaLocation="swdml-shared-4-2.xsd"/>
<xsd:include schemaLocation="swdml-ird-enum-4-2.xsd"/>
<xsd:include schemaLocation="fpml-main-4-2.xsd"/>
<xsd:include schemaLocation="fpml-doc-4-2.xsd"/>
<xsd:include schemaLocation="fpml-ird-4-2.xsd"/>
<xsd:include schemaLocation="swdml-reporting-1-0.xsd"/>
<xsd:element name="SWDML">
<xsd:complexType>
<xsd:complexContent>
<xsd:extension base="SWDML"/>
</xsd:complexContent>
</xsd:complexType>
</xsd:element>
<xsd:complexType name="SWDML">
<xsd:annotation>
<xsd:documentation xml:lang="en">The root element in an SWDML document.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:choice>
<xsd:element name="swShortFormTrade" type="SW_ShortFormTrade">
<xsd:annotation>
<xsd:documentation xml:lang="en">The container for a short form SWDML trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swLongFormTrade" type="SW_LongFormTrade">
<xsd:annotation>
<xsd:documentation xml:lang="en">This container for a long form SWDML trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:choice>
<xsd:element name="swTradeEventReportingDetails" type="SW_TradeEventReportingDetails" minOccurs="0"/>
</xsd:sequence>
<xsd:attribute name="version" type="SW_Version" use="required"/>
</xsd:complexType>
<xsd:complexType name="SW_LongFormTrade">
<xsd:annotation>
<xsd:documentation xml:lang="en">This container for a long form SWDML trade message.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="swOriginatorPartyReference" type="PartyOrTradeSideReference">
<xsd:annotation>
<xsd:documentation xml:lang="en">A pointer style reference to a party identifier defined elsewhere in the SWDML document. The party referenced is the party that entered the trade into the SwapsWire systemI.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swGiveUp" type="SW_GiveUp" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of the give up relationships for a prime brokered transaction. This element will be present if the trade has a trade status of PrimeBrokered.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="novation" type="SW_ConfirmedNovationAgreement" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The Novation Agreement details and any related fee associated with the transfer of the trade. This element will be present if the trade has a trade status of 'Novated' or 'New-Novated'. This is an FpML 4.2 element.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swAllocations" type="SW_Allocations" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of allocation splits that will be applied to the trade.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swStructuredTradeDetails" type="SW_IrdStructuredTradeDetails">
<xsd:annotation>
<xsd:documentation xml:lang="en">The details of the transaction defined in a formal structure.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_IrdStructuredTradeDetails">
<xsd:annotation>
<xsd:documentation xml:lang="en">The details of the transaction defined in a formal structure.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="swProductType" type="SW_IrdProductTypeEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">The product type is the top level categorization for SwapsWire templates.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swTemplateName" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The name of the SwapsWire template associated with the trade. This will be populated by the SwapsWire system.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swParticipantSupplement" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The name of an applicable participant supplement template that was applied to the trade.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:choice>
<xsd:element name="FpML" type="DataDocument">
<xsd:annotation>
<xsd:documentation xml:lang="en">The FpML compliant sub-tree that describes the transaction. This sub-tree uses the FpML DataDocument type.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swOfflineTrade" type="SW_OfflineTrade">
<xsd:annotation>
<xsd:documentation xml:lang="en">The component that describes the details of a rates offline trade.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:choice>
<xsd:element name="swExtendedTradeDetails" type="SW_IrdExtendedTradeDetails">
<xsd:annotation>
<xsd:documentation xml:lang="en">A component for definining additional structured trade details that are not captured within the FpML component of the document.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_OfflineTrade">
<xsd:annotation>
<xsd:documentation>A component for defining details of a rates offline product.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="swOfflineProductType" minOccurs="0"/>
<xsd:element name="swTradeDate" type="xsd:date"/>
<xsd:element name="swLegDetails" type="SW_LegDetails" maxOccurs="2">
<xsd:annotation>
<xsd:documentation xml:lang="en">Details of each leg.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swOptionType" minOccurs="0"/>
<xsd:element name="expirationDate" type="xsd:date" minOccurs="0"/>
<xsd:element name="effectiveDate" type="xsd:date"/>
<xsd:element name="terminationDate" type="xsd:date"/>
<xsd:element name="swAdditionalDetails" minOccurs="0"/>
<xsd:element name="swOriginalTradeReferences" type="SW_OriginalTradeReferences" minOccurs="0"/>
<xsd:element name="party" type="Party" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_LegDetails">
<xsd:sequence>
<xsd:choice minOccurs="0">
<xsd:sequence minOccurs="0">
<xsd:element name="payerPartyReference" type="PartyReference"/>
<xsd:element name="receiverPartyReference" type="PartyReference"/>
</xsd:sequence>
<xsd:sequence minOccurs="0">
<xsd:element name="buyerPartyReference" type="PartyReference"/>
<xsd:element name="sellerPartyReference" type="PartyReference"/>
</xsd:sequence>
</xsd:choice>
<xsd:element name="notional" type="Money"/>
<xsd:element name="fixedRate" type="xsd:decimal" minOccurs="0"/>
<xsd:element name="floatingRateIndex" type="FloatingRateIndex" minOccurs="0"/>
<xsd:element name="indexTenor" type="Interval" minOccurs="0"/>
<xsd:element name="paymentFrequency" type="Interval" minOccurs="0"/>
</xsd:sequence>
<xsd:attribute name="id" type="xsd:ID"/>
</xsd:complexType>
<xsd:complexType name="SW_OriginalTradeReferences">
<xsd:sequence>
<xsd:element name="swOriginalTradeReference" maxOccurs="2">
<xsd:complexType>
<xsd:sequence>
<xsd:element name="partyReference" type="PartyReference"/>
<xsd:element name="swTradeReference" minOccurs="0"/>
<xsd:element name="swTradeDesk" minOccurs="0"/>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_ShortFormTrade">
<xsd:annotation>
<xsd:documentation xml:lang="en">This container for a short form SWDML trade message.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="swOriginatorPartyReference" type="PartyOrTradeSideReference">
<xsd:annotation>
<xsd:documentation xml:lang="en">A pointer style reference to a party identifier defined elsewhere in the SWDML document. The party referenced is the party that entered the trade into the SwapsWire systemI.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swParticipantSupplement" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Participant supplement enforced on the trade</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:choice>
<xsd:element name="swCapFloorParameters" type="SW_CapFloorParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of a short form CapFloor trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swFraParameters" type="SW_FraParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of a short form FRA trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swIrsParameters" type="SW_IrsParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of a short form IRS trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swOisParameters" type="SW_OisParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of a short form OIS trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swSwaptionParameters" type="SW_SwaptionParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of a short form Swaption trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swZCInflationSwapParameters" type="SW_ZCInflationSwapParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of a short form ZCInflationSwap trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swBasisSwapParameters" type="SW_BasisSwapParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details of a short form Basis Swap trade message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:choice>
<xsd:element name="swBackLoadingFlag" type="xsd:boolean" minOccurs="0">
<xsd:annotation>
<xsd:documentation>A flag to identify backloaded trades.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_CapFloorParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">A component for definining a short form Swaption trade message.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="tradeDate" type="xsd:date">
<xsd:annotation>
<xsd:documentation xml:lang="en">Date the trade was executed</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="buyerPartyReference" type="PartyOrTradeSideReference">
<xsd:annotation>
<xsd:documentation xml:lang="en">A pointer style reference to a party identifier defined elsewhere in the SWDML document. The party referenced is the party that entered the trade into the SwapsWire systemI.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="sellerPartyReference" type="PartyOrTradeSideReference">
<xsd:annotation>
<xsd:documentation xml:lang="en">A pointer style reference to a party identifier defined elsewhere in the SWDML document. The party referenced is the party that entered the trade into the SwapsWire systemI.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swPremium" type="SW_Premium" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Swaption premium</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swOptionType" type="SW_CapFloorTypeEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">CapFloor type as 'Cap', 'Floor' or 'Cap Floor Straddle'</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swSwapTermTenor" type="Interval">
<xsd:annotation>
<xsd:documentation xml:lang="en">Swap tenor</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="adjustedEffectiveDate" type="xsd:date" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Adjusted start date</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swUnadjustedTerminationDate" type="xsd:date" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Unadjusted end date</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="rollConvention" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The roll convention</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swStubPosition" type="SW_StubPositionEnum" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Stub position as "Start" or "End"</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swEarlyTerminationProvision" type="SW_EarlyTerminationProvision" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Early termination provision information relating to the Swap Transaction. Either Mandatory or Optional Early Termination can be specified. Note that this element should not be present for a FRA or CapFloor trade. For a Swaption trade the early termination is assumed to apply to the underlying swap.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="notional" type="Money">
<xsd:annotation>
<xsd:documentation xml:lang="en">Notional amount</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swStrikeRate" type="xsd:decimal">
<xsd:annotation>
<xsd:documentation xml:lang="en">The CapFloor strike rate.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="floatingRateIndex" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Optional floating rate index code</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="indexTenor" type="Interval" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The ISDA Designated Maturity, i.e. the tenor of the floating rate.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="paymentFrequency" type="Interval" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The frequency at which regular payment dates occur. If the payment frequency is equal to the frequency defined in the calculation period dates component then one calculation period contributes to each payment amount. If the payment frequency is less frequent than the frequency defined in the calculation period dates component then more than one calculation period will contribute to e payment amount. A payment frequency more frequent than the calculation period frequency or one that is not a multiple of the calculation period frequency is invalid.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="party" type="SW_Party" minOccurs="2" maxOccurs="2">
<xsd:annotation>
<xsd:documentation xml:lang="en">Party identifier of the buyer or seller</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_FraParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">A component for definining a short form FRA trade message.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="tradeDate" type="xsd:date">
<xsd:annotation>
<xsd:documentation xml:lang="en">Date the trade was executed</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swEffectiveDateTenor" type="Interval">
<xsd:annotation>
<xsd:documentation xml:lang="en">Forward date tenor</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swTerminationDateTenor" type="Interval">
<xsd:annotation>
<xsd:documentation xml:lang="en">Forward date tenor</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="rollConvention" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The roll convention</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="buyerPartyReference" type="PartyOrTradeSideReference">
<xsd:annotation>
<xsd:documentation xml:lang="en">A pointer style reference to a party identifier defined elsewhere in the SWDML document. The party referenced is the party that entered the trade into the SwapsWire systemI.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="sellerPartyReference" type="PartyOrTradeSideReference">
<xsd:annotation>
<xsd:documentation xml:lang="en">A pointer style reference to a party identifier defined elsewhere in the SWDML document. The party referenced is the party that entered the trade into the SwapsWire systemI.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="notional" type="Money">
<xsd:annotation>
<xsd:documentation xml:lang="en">Notional amount</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="fixedRate" type="xsd:decimal">
<xsd:annotation>
<xsd:documentation xml:lang="en">Fixed Rate</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="floatingRateIndex" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Optional floating rate index code</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="party" type="SW_Party" minOccurs="2" maxOccurs="2">
<xsd:annotation>
<xsd:documentation xml:lang="en">Party identifier of the buyer or seller</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_IrsParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">A component for definining a short form IRS trade message.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="swProductSubType" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">To suppert Product Subtypes such as "ZeroCouponFixed"</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="tradeDate" type="xsd:date">
<xsd:annotation>
<xsd:documentation xml:lang="en">Date the trade was executed</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swSwapTermTenor" type="Interval">
<xsd:annotation>
<xsd:documentation xml:lang="en">Swap tenor</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="adjustedEffectiveDate" type="xsd:date" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Adjusted start date</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swUnadjustedTerminationDate" type="xsd:date" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Unadjusted end date</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="rollConvention" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The roll convention</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swRollFrequency" type="Interval" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Roll fequencny</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swFloatPaymentFrequency" type="Interval" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Float Payment fequency</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swFixedPaymentFrequency" type="Interval" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Fixed Paymentl fequency</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swCompoundingMethod" type="CompoundingMethodEnum" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Compounding Method</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swStubPosition" type="SW_StubPositionEnum" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Stub position as "Start" or "End"</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swFixedStubLength" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The specification of whether an applicable stub is a short or long stub. This element links the corresponding stub to the fixed leg. Note that this element should not be present for a FRA trade. For a Swaption trade the stub would apply to the underlying swap.</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:simpleContent>
<xsd:extension base="SW_StubLengthEnum"/>
</xsd:simpleContent>
</xsd:complexType>
</xsd:element>
<xsd:element name="swFloatStubLength" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The specification of whether an applicable stub is a short or long stub. This element links the corresponding stub to the floating leg. Note that this element should not be present for a FRA trade. For a Swaption trade the stub would apply to the underlying swap.</xsd:documentation>
</xsd:annotation>
<xsd:complexType>
<xsd:simpleContent>
<xsd:extension base="SW_StubLengthEnum"/>
</xsd:simpleContent>
</xsd:complexType>
</xsd:element>
<xsd:element name="swStubIndexTenor" type="Interval" minOccurs="0" maxOccurs="2">
<xsd:annotation>
<xsd:documentation xml:lang="en">Stub Index Tenors</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swFixedAmounts" type="SW_FixedAmounts">
<xsd:annotation>
<xsd:documentation xml:lang="en">References to payer and receiver in a swap transaction</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swEarlyTerminationProvision" type="SW_EarlyTerminationProvision" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Early termination provision information relating to the Swap Transaction. Either Mandatory or Optional Early Termination can be specified. Note that this element should not be present for a FRA or CapFloor trade. For a Swaption trade the early termination is assumed to apply to the underlying swap.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swAssociatedBonds" type="SW_AssociatedBonds" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The underlying bond details if the swap price was negotiated as a spread over underlying bond reference instruments. Note that this element should not be present for a FRA, OIS, CapFloor or Swaption trade.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="notional" type="Money">
<xsd:annotation>
<xsd:documentation xml:lang="en">Notional amount</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:choice>
<xsd:element name="fixedRate" type="xsd:decimal">
<xsd:annotation>
<xsd:documentation xml:lang="en">Fixed Rate. 0.05 is 5%</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="fixedAmount" type="xsd:decimal">
<xsd:annotation>
<xsd:documentation xml:lang="en">Fixed Amount for a ZC swap</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:choice>
<xsd:element name="floatingRateIndex" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Optional floating rate index code</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="initialRate" type="xsd:decimal" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">First fixing</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="additionalTerms" type="SW_AdditionalTerms" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Additional terms relating to participant supplements</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="party" type="SW_Party" minOccurs="2" maxOccurs="2">
<xsd:annotation>
<xsd:documentation xml:lang="en">Party identifier of the buyer or seller</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:choice>
<xsd:element name="swSettlementProvision" type="SW_SettlementProvision" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Settlement details for a non deliverable currency</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="nonDeliverable" type="xsd:boolean" minOccurs="0">
<xsd:annotation>
<xsd:documentation>Non-Deliverable Indicator</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:choice>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="SW_ZCInflationSwapParameters">
<xsd:annotation>
<xsd:documentation xml:lang="en">A component for defining a short form ZCInflationSwap trade message.</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="tradeDate" type="xsd:date">
<xsd:annotation>
<xsd:documentation xml:lang="en">Date the trade was executed</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swSwapTermTenor" type="Interval">
<xsd:annotation>
<xsd:documentation xml:lang="en">Swap tenor</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="adjustedEffectiveDate" type="xsd:date" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Adjusted start date</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swFixedAmounts" type="SW_FixedAmounts">
<xsd:annotation>
<xsd:documentation xml:lang="en">References to payer and receiver in a swap transaction</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swEarlyTerminationProvision" type="SW_EarlyTerminationProvision" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Early termination provision information relating to the Swap Transaction. Either Mandatory or Optional Early Termination can be specified. Note that this element should not be present for a FRA or CapFloor trade. For a Swaption trade the early termination is assumed to apply to the underlying swap.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="notional" type="Money">
<xsd:annotation>
<xsd:documentation xml:lang="en">Notional amount</xsd:documentation>
</xsd:annotation>
