<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details specific to Variance Swap terminations</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="equitySwap" type="fpml:equitySwap">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details specific to Equity Swap position updates</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="volatilitySwap" type="fpml:varianceSwap">
<xsd:annotation>
<xsd:documentation>Defines details specific to Volatility Swap terminations</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="Accumulator" type="fpml:Accumulator">
<xsd:annotation>
<xsd:documentation>Defines details specific to Equity Accumulator terminations</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="basketSwap" type="fpml:basketSwap">
<xsd:annotation>
<xsd:documentation xml:lang="en">Defines details specific to Equity Basket Swap position updates</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:choice>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
<xsd:element name="positionChangeIndependentAmount" type="fpml:positionChangeIndependentAmount" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">Specifies the independent amount applicable</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swBusinessConductDetails" type="fpml:SW_BusinessConductDetails" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">A component for defining trade details related to business conduct standards that are not captured within the FpML component of the message.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
<xsd:complexType name="positionChangeIndependentAmount">
<xsd:annotation>
<xsd:documentation>Specifies the independent amount applicable</xsd:documentation>
</xsd:annotation>
<xsd:sequence>
<xsd:element name="payerPartyReference">
<xsd:annotation>
<xsd:documentation>The paying party</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="receiverPartyReference">
<xsd:annotation>
<xsd:documentation>The receiving party</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="paymentAmount" type="fpml:independentAmountAndPercentage">
<xsd:annotation>
<xsd:documentation xml:lang="en">A fixed payment and percentage amount.</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
<xsd:complexType name="varianceSwap">
<xsd:annotation>
