<xsd:documentation>The notional amount of the position update.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="newNumberOfOptions" type="xsd:decimal" minOccurs="0">
<xsd:annotation>
<xsd:documentation>The number of options remaining after the position update occurring.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="newNumberOfShares" type="xsd:decimal" minOccurs="0"/>
<xsd:element name="newEquityNotional" type="fpml:Money" minOccurs="0">
