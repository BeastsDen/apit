<xsd:sequence>
<xsd:element name="payerPartyReference">
<xsd:annotation>
<xsd:documentation>The paying party</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="receiverPartyReference">
<xsd:annotation>
<xsd:documentation>The receiving party</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="paymentAmount" type="fpml:Money" minOccurs="0">
<xsd:annotation>
<xsd:documentation xml:lang="en">The currency amount of the payment.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="paymentDate" type="xsd:date" minOccurs="0">
<xsd:annotation>
<xsd:documentation>The payment date of the equity payment</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
</xsd:element>
<xsd:element name="optionStrategyDetails" type="fpml:OptionStrategyDetails" minOccurs="0" maxOccurs="unbounded"/>
</xsd:sequence>
</xsd:complexType>
