<xsd:simpleType name="SW_ExecutionMethodEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">The values used to specify per side Excecution Method information.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="Voice">
<xsd:annotation>
<xsd:documentation xml:lang="en">Voice Execution Method.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="Electronic">
<xsd:annotation>
<xsd:documentation xml:lang="en">Electronic Execution Method.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="SW_ReplacementReasonEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">The values used to specify trade id replacement reasons.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="IndexTransitionReplacedTrade">
<xsd:annotation>
<xsd:documentation xml:lang="en">IBOR transition Replaced Trade ref</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="IndexTransitionReplacedByTrade">
<xsd:annotation>
<xsd:documentation xml:lang="en">IBOR transition Replaced by Trade ref</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="SW_TradeIdTypeEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">The values used to specify replacement trade id  type </xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="MarkitWireId">
<xsd:annotation>
<xsd:documentation xml:lang="en">MarkitWire ID"</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="OtherId">
<xsd:annotation>
<xsd:documentation xml:lang="en">Other ID</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="SW_FallbacksSupplementEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">A field to explicitly incorporate Supplement 70 to the 2006 IDSA Definitions into New-Novated swap transactions</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="Incorporate">
<xsd:annotation>
<xsd:documentation xml:lang="en">Incorporate Supplement 70 to the 2006 IDSA Definitions into New-Novated swap transaction</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="Unspecified">
<xsd:annotation>
<xsd:documentation xml:lang="en">Incorporation of Supplement 70 to the 2006 IDSA Definitions is not explicitly specified</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="SW_EconomicAmendmentTypeEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">Type of economic amendment being submitted on the trade</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="Index Transition">
<xsd:annotation>
<xsd:documentation xml:lang="en">If Transition from IRS to OIS is done via UI, swEconomicAmendmentType should be defaulted to Index Transition</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="SW_LinkedTradeIdTypeEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">The values used to specify replacement trade id  type </xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="MarkitWire ID">
<xsd:annotation>
<xsd:documentation xml:lang="en">MarkitWire ID"</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="CCP Trade ID">
<xsd:annotation>
<xsd:documentation xml:lang="en">CCP Trade ID</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="Internal Trade ID">
<xsd:annotation>
<xsd:documentation xml:lang="en">Internal Trade ID</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="SW_FallbacksMechanismEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">Indication of the fallback mechanism triggered on the trade following the cessation of a Floating Rate Index </xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="ISDA">
<xsd:annotation>
<xsd:documentation xml:lang="en">In case FallbacksMechanism is based on ISDA guidelines"</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="Bespoke">
<xsd:annotation>
<xsd:documentation xml:lang="en">In case FallbacksMechanism is customized</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
</xsd:schema>
