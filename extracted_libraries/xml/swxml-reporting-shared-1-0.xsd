<xsd:complexType name="SW_AllocationReportingDetails">
<xsd:sequence>
<xsd:element name="swJurisdiction" type="SW_Jurisdiction" minOccurs="0"/>
<xsd:sequence minOccurs="0">
<xsd:element name="swSupervisoryBodyCategory" type="SW_SupervisoryBodyCategory" minOccurs="0">
<xsd:annotation>
<xsd:documentation>CRT14: Regulator Type</xsd:documentation>
<xsd:documentation>Categorisation of the
supervisorybody.</xsd:documentation>
<xsd:documentation>Requirement: CRT14 Will always present for Dodd-Frank
Will never be present for ODRF</xsd:documentation>
<xsd:documentation/>
</xsd:annotation>
</xsd:element>
<xsd:element name="swReportableLocations" type="SW_ReportableLocations" minOccurs="0" maxOccurs="2"/>
</xsd:sequence>
<xsd:element name="swUniqueTransactionId" type="SW_UniqueTradeIdentifier" minOccurs="0">
<xsd:annotation>
<xsd:documentation>USI that will be used to identify the resulting
split</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swObligatoryReporting" type="xsd:boolean" minOccurs="0">
<xsd:annotation>
<xsd:documentation>CRT1 (for the split) The value of this is NOT(the value of
"Within Jurisdiction").</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swReportingCounterpartyReference" type="SW_PartyReference" minOccurs="0">
<xsd:annotation>
<xsd:documentation>An occurrence for each party identified as a reporting party,
for the split, within the relevant reporting regime. For Dodd-Frank
reporting there will always be 1 occurrence</xsd:documentation>
<xsd:documentation>Requirement: CRT2</xsd:documentation>
<xsd:documentation>Dodd-Frank: Always 1 occurence</xsd:documentation>
<xsd:documentation>ODRF: Always 2 occurences ("partyA" and
"partyB")</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:sequence minOccurs="0">
<xsd:element name="swRegulatoryReportable" type="xsd:boolean" minOccurs="0">
<xsd:annotation>
<xsd:documentation>A flag for MIFID Transaction Reportable to be passed on
each allocated trade (Boolean)</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swTransparencyReportable" type="xsd:boolean" minOccurs="0">
<xsd:annotation>
<xsd:documentation>MiFID field: Transparency Reportable (Boolean) to be
passed on each allocated trade</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
<xsd:element name="swMIFIDTransactionIdentifier" type="SW_MIFIDTransactionIdentifier" minOccurs="0">
<xsd:annotation>
<xsd:documentation>Uniquely identifies a transaction for MIFID reporting
purposes.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swReportForCounterparty" type="xsd:boolean" minOccurs="0">
<xsd:annotation>
<xsd:documentation>Corporate sector of the counterparty required for ESMA and
FCA reporting for NFC-</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
</xsd:complexType>
