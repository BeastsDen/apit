<xsd:annotation>
<xsd:documentation>Output the Notation associated with the Package Price/Spread</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:element name="swCurrency" type="xsd:normalizedString" minOccurs="0">
<xsd:annotation>
<xsd:documentation>Output the Currency associated with the Package Price/Spread</xsd:documentation>
</xsd:annotation>
</xsd:element>
</xsd:sequence>
