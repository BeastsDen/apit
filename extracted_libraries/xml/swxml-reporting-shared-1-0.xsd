<xsd:sequence>
<xsd:element name="swEventIdentifier" type="SW_EventIdentifier" minOccurs="0">
<xsd:annotation>
<xsd:documentation>CRT11: GTR Report Event ID Event id for the event causing the
creation of the trade version being represented.</xsd:documentation>
</xsd:annotation>
</xsd:element>
<xsd:choice minOccurs="0">
<xsd:element name="swBulkProcessingEventId" type="SW_BulkProcessingEvent">
<xsd:annotation>
<xsd:documentation>CRT12: GTR Blk Event Processing ID </xsd:documentation>
</xsd:annotation>
</xsd:element>
