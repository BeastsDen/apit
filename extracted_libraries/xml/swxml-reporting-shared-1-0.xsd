<xsd:annotation>
<xsd:documentation>default:"http://www.fpml.org/coding-scheme/execution-venue-type"</xsd:documentation>
</xsd:annotation>
</xsd:attribute>
</xsd:extension>
</xsd:simpleContent>
</xsd:complexType>
<xsd:complexType name="SW_IssuerId">
