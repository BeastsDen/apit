<?xml version="1.0"?>
<xsd:schema xmlns="http://www.fpml.org/2009/FpML-4-6" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="SW_20_1_460930" targetNamespace="http://www.fpml.org/2009/FpML-4-6" elementFormDefault="qualified" attributeFormDefault="unqualified">
<xsd:simpleType name="SW_CdsProductTypeEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">The product type is the top level categorization for SwapsWire templates.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="CDS">
<xsd:annotation>
<xsd:documentation xml:lang="en">A credit default swap.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="CDS Index">
<xsd:annotation>
<xsd:documentation xml:lang="en">A credit default swap index.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="CDS Master">
<xsd:annotation>
<xsd:documentation xml:lang="en">A credit default swap confirmed under a master credit derivatives confirmation agreement.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="CDS Matrix">
<xsd:annotation>
<xsd:documentation xml:lang="en">A credit default swap confirmed under the ISDA Physical Settlement Matrix.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="SW_FrontloadEnum">
<xsd:annotation>
<xsd:documentation xml:lang="en">A field to indicate the frontloading category of a trade under EMIR regulation by ESMA.</xsd:documentation>
</xsd:annotation>
<xsd:restriction base="xsd:token">
<xsd:enumeration value="Category1">
<xsd:annotation>
<xsd:documentation xml:lang="en">The trade falls under frontloading Category 1 as per the criteria laid down by ESMA.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="Category2">
<xsd:annotation>
<xsd:documentation xml:lang="en">The trade falls under frontloading Category 2 as per the criteria laid down by ESMA.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
<xsd:enumeration value="NotApplicable">
<xsd:annotation>
<xsd:documentation xml:lang="en">The category of trade does exist but dos not match the criteria laid down by ESMA for category 1 or 2 frontloading.</xsd:documentation>
</xsd:annotation>
</xsd:enumeration>
</xsd:restriction>
</xsd:simpleType>
</xsd:schema>
